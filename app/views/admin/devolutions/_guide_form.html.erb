<div class="row pb-3">
  <div class="col-12">
    <%= form_for [:admin, devolution] do |f| %>
      <div class="form-group">
        <%= f.label :parcel %>
        <%= f.select :parcel, Order::PARCELS, {}, { class: 'form-control', required: true } %>
      </div>
      <div class="form-group">
        <%= f.label :guide_id %>
        <%= f.text_field :guide_id, class: 'form-control', required: true, maxlenght: 100 %>
      </div>
      <%= f.submit t('labels.save'), class: 'btn btn-primary' %>
    <% end if can? :resend, devolution %>
  </div>
</div>
