<% provide :title, OriginState.model_name.human(count: 2) %>

<div class="row">
  <div class="col-md-12">
    <div class="card">
      <div class="card-header">
        <%= content_tag :h2, yield(:title), class: 'card-title' %>
      </div>
      <div class="card-body">
        <div class="d-flex flex-wrap flex-md-row flex-column mb-4">
          <%= render @origin_states %>
        </div>
      </div>
    </div>
  </div>
</div>
